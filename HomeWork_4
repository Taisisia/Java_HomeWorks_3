package org.example._20240523h;

import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Введите первое число");
        int numFirst = sc.nextInt();
        System.out.println("Введите второе число");

        int numSecond = sc.nextInt();
        int resultSum = numFirst + numSecond;
        System.out.println(resultSum);

        int resultSub = numFirst - numSecond;
        System.out.println(resultSub);

        int resultMultiplication = numFirst * numSecond;
        System.out.println(resultMultiplication);

        int resultDivision = numFirst / numSecond;
        System.out.println(resultDivision);
    }

    }

